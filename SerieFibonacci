paquete  seriefibonacci ;

import  java.util.Scanner ;

 clase  pública SerieFibonacci {

    public  static  void  main ( String [] args ) {

        Entrada del escáner =  nuevo  escáner ( System . In);
        Sistema . fuera . println ( " Ingrese el número de resultados de la serie Fibonacci: " );
        largo limite = entrada . nextLong ();
        
        Sistema . fuera . println ( " n los Primeros "  + limite +  " de la serie Fibonacci son: " );
        fibonacci (limite, 0 , 1 );
    }

    pública  estática  vacío  de Fibonacci ( tiempo  limite , a largo  anterior , siempre  real ) {
        si (límite >  0 ) {
             
            Sistema . fuera . println ( "  "  + actual);
            fibonacci (limite - 1 , actual, anterior + actual);
        }
    }
}
